<h1><%= title %> </h1>
<ul>
  <% genre_list.sort(function(a, b) {  %>
  <%  let textA = a.name.toUpperCase();  %>
  <%     let textB = b.name.toUpperCase();  %>
  <%  return (textA < textB) ? -1 : (textA> textB) ? 1 : 0;}); %>
  <% if (genre_list.length > 0) { %>
  <% for(let genre of genre_list){ %>
  <li>
    <a href='<%=  genre.url %>'><%= genre.name %> </a>
  </li>
  <% } %>
  <% }else {%>
  <li>
    There are no Genres.
  </li>
  <% } %>
</ul>